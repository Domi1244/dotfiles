# Check if z directory exists
ok directory ${HOME}/.z-sh

# Check if .z-sh/z.sh exists
if [[ ! -f ${HOME}/.z-sh/z.sh ]]; then
    echo "missing: ~/.z-sh/z.sh"
    wget https://raw.githubusercontent.com/rupa/z/master/z.sh -O ${HOME}/.z-sh/z.sh
    echo "ok: ~/.z-sh/z.sh"
else
    echo "ok: ~/.z-sh/z.sh"
fi
